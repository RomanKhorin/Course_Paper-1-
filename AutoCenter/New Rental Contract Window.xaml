        <TextBlock HorizontalAlignment="Left" Margin="25,212,0,0" TextWrapping="Wrap" Text="Date Of Begin" VerticalAlignment="Top" Height="23" Width="76"/>
        <DatePicker Name="date_of_end_datepicker" HorizontalAlignment="Left" Margin="133,208,0,0" VerticalAlignment="Top" Width="120" FirstDayOfWeek="Monday" SelectedDateFormat="Long"/>

    </Grid>
